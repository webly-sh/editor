{
    "name": "@webly/editor",
    "author": "Kevin Sundar Raj (@kevtechi)",
    "version": "0.1.2",
    "exports": {
        ".": "./webly.plugin.json",
        "./dist": "./dist.tar.gz"
    },
    "license": "GPL-3.0",
    "vendor": true,
    "tasks": {
        "dev": "deno run -A npm:vite",
        "build": "deno check **/*.ts && deno run -A npm:vite build"
    },
    "publish": {
        "include": [
            "deno.json",
            "LICENSE",
            "README.md",
            "webly.plugin.json",
            "dist.tar.gz"
        ]
    }
}